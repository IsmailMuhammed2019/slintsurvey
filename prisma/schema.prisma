generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SurveyResponse {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  fullName       String
  email          String
  phone          String?
  location       String?
  profile        String[]
  cluster        String[]
  fundingNeed    String?
  fundingRange   String?
  cardInterest   String?
  timeCommitment String?
  answers        Json
}
